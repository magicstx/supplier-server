  "scripts": {
    "r": "NODE_ENV=development node -r @swc-node/register -r dotenv/config",
    "dev": "NODE_ENV=development yarn r scripts/server.ts",
    "start": "node -r @swc-node/register scripts/server.ts",
    "server:dev": "yarn r scripts/server.ts",
    "server:dev:watch": "nodemon --verbose -x 'yarn server:dev'",
    "typecheck": "tsc --noEmit",
    "generate-keys": "yarn r scripts/generate-keys.ts",
    "check-config": "yarn r scripts/check-config.ts",
    "register-supplier": "yarn r scripts/register-supplier.ts",
    "eslint:ci": "eslint 'src/**/*.ts' 'scripts/**/*.ts'",
    "lint:prettier": "prettier --check \"{src,scripts}/**/*.{ts,tsx}\"",
    "lint": "yarn eslint:ci && yarn lint:prettier",
    "flush": "yarn r scripts/flush-redis.ts",
    "devnet": "yarn flush && yarn dev",
    "copy-types": "cp -f -r ../bridge/common/clarigen/ src/clarigen"
  },
  "license": "ISC",
